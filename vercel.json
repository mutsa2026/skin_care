{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "public",
  "public": "public",
  "routes": [
    {
      "src": "^/build/(.*)",
      "dest": "/public/build/$1"
    },
    {
      "src": "^/public/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ]
}