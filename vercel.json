{
  "version": 2,
  "buildCommand": "npm run build",
  "public": "public",
  "functions": {
    "public/**.php": {
      "runtime": "vercel-php@0.6.1"
    }
  },
  "routes": [
    {
      "src": "^/build/(.*)",
      "dest": "/public/build/$1"
    },
    {
      "src": "^/public/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/public/index.php"
    }
  ]
}